{
  "name": "ts-seed-api",
  "version": "1.0.0",
  "description": "Typescript Seed API for all projects to handle CRUD operations",
  "main": "index.js",
  "scripts": {
    "start": "env TZ='Asia/Dubai' ./node_modules/.bin/tsc-watch --noClear --onSuccess \"node ./dist/index.js\"",
    "compile": "./node_modules/.bin/tsc",
    "test": "./node_modules/mocha/bin/mocha -r ts-node/register ./functional-test/*.test.ts",
    "lint": "./node_modules/.bin/tslint --project tslint.json 'endpoints/*.ts' 'endpoints/**/*.ts' 'lib/*.ts' 'lib/**/*.ts' 'functional-test/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/10ten-me/ts-seed-api.git"
  },
  "keywords": [
    "typescript",
    "API",
    "CRUD"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/10ten-me/ts-seed-api/issues"
  },
  "homepage": "https://github.com/10ten-me/ts-seed-api#readme",
  "dependencies": {
    "@sentry/node": "5.12.3",
    "ajv": "6.11.0",
    "axios": "0.19.2",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-fileupload": "1.1.9",
    "jsonwebtoken": "8.5.1",
    "pg": "7.18.1",
    "pg-native": "3.0.0",
    "ts-node": "8.6.2",
    "tsc-watch": "4.1.0",
    "typescript": "3.7.5",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.9",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/express-fileupload": "1.1.1",
    "@types/faker": "4.1.9",
    "@types/jsonwebtoken": "8.3.7",
    "@types/mocha": "7.0.1",
    "@types/node": "13.7.1",
    "@types/pg": "7.14.1",
    "@types/supertest": "2.0.8",
    "chai": "4.2.0",
    "faker": "4.1.0",
    "mocha": "7.0.1",
    "supertest": "4.0.2",
    "ts-loader": "6.2.1",
    "tslint": "6.0.0"
  }
}
