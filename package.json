{
   "name": "ts-seed-api",
   "version": "1.0.0",
   "description": "Typescript Seed API for all projects to handle CRUD operations",
   "main": "index.js",
   "scripts": {
      "start": "env TZ='Asia/Dubai' npx tsc-watch --noClear --onSuccess \"node ./dist/index.js --port 8084\"",
      "compile": "npm run swagger-generate && ./node_modules/.bin/tsc --alwaysStrict",
      "docker": "docker-compose up --build",
      "clean": "rm -rf node_modules && rm package-lock.json",
      "test": "npx mocha -r ts-node/register ./functional-test/*.test.ts",
      "lint": "npx eslint -c .eslintrc.js --ext .ts 'index.ts' 'endpoints/*.ts' 'endpoints/**/*.ts' 'lib/*.ts' 'lib/**/*.ts' 'functional-test/*.ts'",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "greenkeeping": "npx ncu -u",
      "swagger-generate": "npx swagger-cli bundle --type=yaml --outfile=swagger/swagger-generated.yml swagger/swagger.yml",
      "swagger-check": "npx swagger-cli validate swagger/swagger.yml"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/pierrick-libert/ts-seed-api.git"
   },
   "keywords": [
      "typescript",
      "API",
      "CRUD"
   ],
   "author": "Pierrick Libert <pierrick.libert@gmail.com>",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/pierrick-libert/ts-seed-api/issues"
   },
   "homepage": "https://github.com/pierrick-libert/ts-seed-api#readme",
   "dependencies": {
      "@apidevtools/swagger-cli": "4.0.4",
      "@sentry/node": "7.0.0",
      "ajv": "8.11.0",
      "axios": "0.27.2",
      "cors": "2.8.5",
      "express": "4.18.1",
      "express-fileupload": "1.4.0",
      "i18n": "0.15.0",
      "jsonwebtoken": "8.5.1",
      "pg": "8.7.3",
      "reflect-metadata": "0.1.13",
      "swagger-ui-express": "4.4.0",
      "ts-node": "10.8.0",
      "tsc-watch": "5.0.3",
      "typeorm": "0.3.6",
      "typescript": "4.7.2",
      "winston": "3.7.2",
      "yamljs": "0.3.0",
      "yargs": "17.5.1"
   },
   "devDependencies": {
      "@types/chai": "4.3.1",
      "@types/cors": "2.8.12",
      "@types/express": "4.17.13",
      "@types/express-fileupload": "1.2.2",
      "@types/faker": "6.6.8",
      "@types/i18n": "0.13.2",
      "@types/jsonwebtoken": "8.5.8",
      "@types/mocha": "9.1.1",
      "@types/node": "17.0.36",
      "@types/pg": "8.6.5",
      "@types/supertest": "2.0.12",
      "@types/swagger-ui-express": "4.1.3",
      "@types/yamljs": "0.2.31",
      "@types/yargs": "17.0.10",
      "@typescript-eslint/eslint-plugin": "5.27.0",
      "@typescript-eslint/eslint-plugin-tslint": "5.27.0",
      "@typescript-eslint/parser": "5.27.0",
      "chai": "4.3.6",
      "eslint": "8.16.0",
      "eslint-config-prettier": "8.5.0",
      "eslint-plugin-import": "2.26.0",
      "faker": "^5.5.3",
      "mocha": "10.0.0",
      "npm-check-updates": "13.0.3",
      "supertest": "6.2.3",
      "ts-loader": "9.3.0",
      "ts-node": "10.8.0"
   }
}
