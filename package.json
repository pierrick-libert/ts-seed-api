{
   "name": "ts-seed-api",
   "version": "1.0.0",
   "description": "Typescript Seed API for all projects to handle CRUD operations",
   "main": "index.js",
   "scripts": {
      "start": "env TZ='Asia/Dubai' npx tsc-watch --noClear --onSuccess \"node ./dist/index.js --port 8084\"",
      "compile": "npm run swagger-generate && ./node_modules/.bin/tsc --alwaysStrict",
      "docker": "docker-compose up --build",
      "clean": "rm -rf node_modules && rm package-lock.json",
      "test": "npx mocha -r ts-node/register ./functional-test/*.test.ts",
      "lint": "npx eslint -c .eslintrc.js --ext .ts 'index.ts' 'endpoints/*.ts' 'endpoints/**/*.ts' 'lib/*.ts' 'lib/**/*.ts' 'functional-test/*.ts'",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "greenkeeping": "npx ncu -u",
      "swagger-generate": "npx swagger-cli bundle --type=yaml --outfile=swagger/swagger-generated.yml swagger/swagger.yml",
      "swagger-check": "npx swagger-cli validate swagger/swagger.yml"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/pierrick-libert/ts-seed-api.git"
   },
   "keywords": [
      "typescript",
      "API",
      "CRUD"
   ],
   "author": "Pierrick Libert <pierrick.libert@gmail.com>",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/pierrick-libert/ts-seed-api/issues"
   },
   "homepage": "https://github.com/pierrick-libert/ts-seed-api#readme",
   "dependencies": {
      "@apidevtools/swagger-cli": "4.0.4",
      "@sentry/node": "7.93.0",
      "ajv": "8.12.0",
      "axios": "1.6.5",
      "cors": "2.8.5",
      "express": "4.18.2",
      "express-fileupload": "1.4.3",
      "i18n": "0.15.1",
      "jsonwebtoken": "9.0.2",
      "pg": "8.11.3",
      "reflect-metadata": "0.2.1",
      "swagger-ui-express": "5.0.0",
      "ts-node": "10.9.2",
      "tsc-watch": "6.0.4",
      "typeorm": "0.3.19",
      "typescript": "5.3.3",
      "winston": "3.11.0",
      "yamljs": "0.3.0",
      "yargs": "17.7.2"
   },
   "devDependencies": {
      "@types/chai": "4.3.11",
      "@types/cors": "2.8.17",
      "@types/express": "4.17.21",
      "@types/express-fileupload": "1.4.4",
      "@types/faker": "6.6.8",
      "@types/i18n": "0.13.10",
      "@types/jsonwebtoken": "9.0.5",
      "@types/mocha": "10.0.6",
      "@types/node": "20.11.0",
      "@types/pg": "8.10.9",
      "@types/supertest": "6.0.2",
      "@types/swagger-ui-express": "4.1.6",
      "@types/yamljs": "0.2.34",
      "@types/yargs": "17.0.32",
      "@typescript-eslint/eslint-plugin": "6.18.1",
      "@typescript-eslint/eslint-plugin-tslint": "6.18.1",
      "@typescript-eslint/parser": "6.18.1",
      "chai": "5.0.0",
      "eslint": "8.56.0",
      "eslint-config-prettier": "9.1.0",
      "eslint-plugin-import": "2.29.1",
      "faker": "^6.6.6",
      "mocha": "10.2.0",
      "npm-check-updates": "16.14.12",
      "supertest": "6.3.3",
      "ts-loader": "9.5.1",
      "ts-node": "10.9.2"
   }
}
